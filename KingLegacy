  pcall(setfpscap, SaveSettings["Setting"]["FpsCap"])
  pcall(set_fps_cap, SaveSettings["Setting"]["FpsCap"])
  else
    pcall(setfpscap,240)
  pcall(set_fps_cap,240)
  end
  end)
